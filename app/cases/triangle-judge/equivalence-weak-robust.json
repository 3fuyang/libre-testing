{
  "cases": [
    {
      "input": [10, 10, 20],
      "expected": "所给三边数据不能构成三角形"
    },
    {
      "input": [10, 10, 10],
      "expected": "该三角形是等边三角形"
    },
    {
      "input": [10, 10, 15],
      "expected": "该三角形是等腰三角形"
    },
    {
      "input": [10, 15, 20],
      "expected": "该三角形是普通三角形"
    },
    {
      "input": [-1, 10, 20],
      "expected": "边长数值越界"
    },
    {
      "input": [201, 10, 10],
      "expected": "边长数值越界"
    },
    {
      "input": [10, -1, 15],
      "expected": "边长数值越界"
    },
    {
      "input": [10, 201, 15],
      "expected": "边长数值越界"
    },
    {
      "input": [10, 10, -1],
      "expected": "边长数值越界"
    },
    {
      "input": [10, 15, 201],
      "expected": "边长数值越界"
    }
  ]
}
